    <add key="NODEID" value="2" />
    <add key="JOBNAME" value="3" />
    <add key="DESCRIPTION" value="4" />
    <add key="RUN_AS" value="5" />
    <add key="MEMNAME" value="6" />
    <add key="MEMLIB" value="7" />
    <!--<add key="VARIABLE" value="8|N|VALUE" />-->
    <add key="TIMEFROM" value="11" />
    <!--<add key="ON" value="12|N|MESSAGE" />-->
    <add key="VARIABLE-NAME|VALUE" value="8;%%PARM|%%FileWatch-FILE_PATH|%%FileWatch-TIME_LIMIT|%%FileWatch-MIN_AGE" />
    <add key="ON-CODE|MESSAGE" value="12;NOTOK" />
  </ControlMDataColumn>
</configuration>
