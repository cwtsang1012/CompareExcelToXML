<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ControlMDataColumn" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="FileType" value ="ControlM"/>
    
    <add key="ExcelFilePath" value="C:\ccbaprog\Grace\CompareExcelToXML_Doc\rubbish.xlsx"/>
    <add key="WorksheetName" value="PDN  job Definition Table"/>
    <add key="SectionName" value="SECG"/>
    <add key="ExcludeColumns" value="1|9|10"/>

    <add key="XMLFilePath" value="C:\ccbaprog\Grace\CompareExcelToXML_Doc\1-10-2019_SECG.xml"/>
    <add key="OutputFilePath" value="C:\ccbaprog\Grace\CompareExcelToXML_Doc\Difference_report.xlsx"/>
  </appSettings>
  <ControlMDataColumn>
    <!-- key = attribute name / node_attribute name-->
    <!-- value = Column No in excel (start from 0)-->
